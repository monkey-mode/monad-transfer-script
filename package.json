{
    "name": "monad-transfer-script",
    "version": "1.0.0",
    "description": "Script to perform transfer transactions on Monad Testnet",
    "main": "transfer.js",
    "scripts": {
        "start": "bun run transfer.js",
        "transfer": "bun run transfer.js",
        "ping-pong": "bun run ping-pong-transfer.js",
        "recover": "bun run ping-pong-transfer.js --recover",
        "dev": "bun --watch transfer.js",
        "dev-ping-pong": "bun --watch ping-pong-transfer.js",
        "test": "bun run test-setup.js",
        "install-bun": "bash install.sh"
    },
    "dependencies": {
        "ethers": "^6.8.1",
        "dotenv": "^16.3.1"
    },
    "keywords": [
        "monad",
        "ethereum",
        "transfer",
        "blockchain"
    ],
    "author": "",
    "license": "MIT"
}